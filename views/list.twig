<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <table border="1">
      <thead>
        <th>Label</th>
        <th>Quantity</th>
        <th>Price</th>
        <th>User</th>
        <th>Show</th>
        <th>Delete</th>
      </thead>

      <tbody>
        {% for item in produit %}
        <tr>
          <td>{{ item.label }}</td>
          <td>{{ item.quantity }}</td>
          <td>{{ item.price }}</td>
          <td>{{ item.user }}</td>
          <td><a href="/users/products/{{ item._id }}">show</a></td>
          <td><a href="/users/products11/delete/{{ item._id }}">delete</a></td>
        </tr>
        {% endfor %} 
      </tbody>
    </table>
  </body>
</html>
